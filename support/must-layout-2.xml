<layout-specification>
  <levels>
    <level order="0" size="2" place-name="mpi_app">
      <analyses>
        <analysis name="DatatypePredefs" group="MUST_Resource"/>
        <analysis name="FinalizeNotify" group="MUST_Base"/>
        <analysis name="MpiTypeArt" group="MUST_MpiTypeChecks"/>
      </analyses>
    </level>
    <level order="1" size="1" place-name="mpi_place">
      <analyses>
        <analysis name="MsgLoggerJson" group="MUST_Base"/>
      </analyses>
    </level>
  </levels>
  <communications>
    <default>
      <comm-strategy name="CStratSimpleUp">
        <settings></settings>
      </comm-strategy>
      <comm-protocol name="CProtMpiSplited">
        <settings></settings>
      </comm-protocol>
    </default>
    <connection top-level="1" bottom-level="0">
      <comm-strategy name="CStratSimpleUp">
        <settings></settings>
      </comm-strategy>
      <comm-protocol name="CProtMpiSplited">
        <settings></settings>
      </comm-protocol
>    </connection>
  </communications>
</layout-specification>
